{
  "name": "@nodevisor/docker",
  "description": "Module provides ability to manage Docker containers",
  "version": "0.0.70",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "prettier": "prettier --write src/**/*.ts test/**/*.ts",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:watch": "npm run build:esm:watch & npm run build:cjs:watch",
    "build:esm": "tsc -p tsconfig.json",
    "build:esm:watch": "tsc -p tsconfig.json --watch",
    "build:cjs": "tsc -p tsconfig-cjs.json",
    "build:cjs:watch": "tsc -p tsconfig-cjs.json --watch",
    "test": "jest --runInBand",
    "valid": "tsc --noEmit"
  },
  "dependencies": {
    "@nodevisor/builder": "^0.0.70",
    "@nodevisor/cluster": "^0.0.70",
    "@nodevisor/core": "^0.0.70",
    "@nodevisor/endpoint": "^0.0.70",
    "@nodevisor/fs": "^0.0.70",
    "@nodevisor/groups": "^0.0.70",
    "@nodevisor/packages": "^0.0.70",
    "@nodevisor/registry": "^0.0.70",
    "@nodevisor/services": "^0.0.70",
    "@nodevisor/ufw": "^0.0.70",
    "lodash": "^4.17.21",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "5.7.2"
  },
  "gitHead": "cf9df5ad6ceb25bfa7dfdc1c42f8b3fff149abb6"
}
