{
  "name": "@nodevisor/cluster",
  "description": "Module provides ability to manage cluster of services",
  "version": "0.0.83",
  "license": "SEE LICENSE IN LICENSE",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "prettier": "prettier --write src/**/*.ts test/**/*.ts",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:watch": "npm run build:esm:watch & npm run build:cjs:watch",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig-cjs.json",
    "build:esm:watch": "tsc -p tsconfig.json --watch",
    "build:cjs:watch": "tsc -p tsconfig-cjs.json --watch",
    "test": "jest --passWithNoTests",
    "valid": "tsc --noEmit"
  },
  "dependencies": {
    "@nodevisor/auth": "workspace:*",
    "@nodevisor/authorized-keys": "workspace:*",
    "@nodevisor/builder": "workspace:*",
    "@nodevisor/shell": "workspace:*",
    "@nodevisor/endpoint": "workspace:*",
    "@nodevisor/packages": "workspace:*",
    "@nodevisor/registry": "workspace:*",
    "@nodevisor/services": "workspace:*",
    "@nodevisor/ssh": "workspace:*",
    "@nodevisor/ufw": "workspace:*",
    "@nodevisor/users": "workspace:*",
    "contexta": "^1.0.1",
    "lodash": "^4.17.23"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.2.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "5.9.3"
  },
  "gitHead": "cf9df5ad6ceb25bfa7dfdc1c42f8b3fff149abb6"
}
