{
  "name": "nodevisor",
  "description": "Server Management and Scalable Application Deployment",
  "version": "0.0.45",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "prettier": "prettier --write src/**/*.ts test/**/*.ts",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig-cjs.json",
    "test": "jest --passWithNoTests",
    "valid": "tsc --noEmit"
  },
  "dependencies": {
    "@nodevisor/auth": "^0.0.45",
    "@nodevisor/authorized-keys": "^0.0.45",
    "@nodevisor/aws": "^0.0.45",
    "@nodevisor/builder": "^0.0.45",
    "@nodevisor/cluster": "^0.0.45",
    "@nodevisor/core": "^0.0.45",
    "@nodevisor/docker": "^0.0.45",
    "@nodevisor/endpoint": "^0.0.45",
    "@nodevisor/env": "^0.0.45",
    "@nodevisor/fs": "^0.0.45",
    "@nodevisor/groups": "^0.0.45",
    "@nodevisor/os": "^0.0.45",
    "@nodevisor/packages": "^0.0.45",
    "@nodevisor/pwsh": "^0.0.45",
    "@nodevisor/registry": "^0.0.45",
    "@nodevisor/schema": "^0.0.45",
    "@nodevisor/services": "^0.0.45",
    "@nodevisor/ssh": "^0.0.45",
    "@nodevisor/ufw": "^0.0.45",
    "@nodevisor/users": "^0.0.45"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "5.7.2"
  },
  "gitHead": "cf9df5ad6ceb25bfa7dfdc1c42f8b3fff149abb6"
}
