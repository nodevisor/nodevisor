{
  "name": "nodevisor",
  "description": "TypeScript Infrastructure Automation Platform",
  "version": "0.0.82",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodevisor/nodevisor.git",
    "directory": "packages/nodevisor"
  },
  "homepage": "https://nodevisor.io",
  "bugs": {
    "url": "https://github.com/nodevisor/nodevisor/issues"
  },
  "keywords": [
    "infrastructure",
    "typescript",
    "automation",
    "docker",
    "devops",
    "deployment"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "prettier": "prettier --write src/**/*.ts test/**/*.ts",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig-cjs.json",
    "test": "jest --passWithNoTests",
    "valid": "tsc --noEmit"
  },
  "dependencies": {
    "@nodevisor/auth": "workspace:*",
    "@nodevisor/authorized-keys": "workspace:*",
    "@nodevisor/aws": "workspace:*",
    "@nodevisor/builder": "workspace:*",
    "@nodevisor/cluster": "workspace:*",
    "@nodevisor/shell": "workspace:*",
    "@nodevisor/docker": "workspace:*",
    "@nodevisor/endpoint": "workspace:*",
    "@nodevisor/env": "workspace:*",
    "@nodevisor/fs": "workspace:*",
    "@nodevisor/groups": "workspace:*",
    "@nodevisor/os": "workspace:*",
    "@nodevisor/packages": "workspace:*",
    "@nodevisor/pwsh": "workspace:*",
    "@nodevisor/registry": "workspace:*",
    "@nodevisor/schema": "workspace:*",
    "@nodevisor/services": "workspace:*",
    "@nodevisor/ssh": "workspace:*",
    "@nodevisor/ufw": "workspace:*",
    "@nodevisor/users": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "5.9.3"
  },
  "gitHead": "cf9df5ad6ceb25bfa7dfdc1c42f8b3fff149abb6"
}
